<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings xdt:Transform="InsertIfMissing">
		<!-- Service Bus specific app setings for receiving messaging connections -->
		<add key="Microsoft.ServiceBus.ConnectionString.Receive" value="Endpoint=sb://[your namespace].servicebus.windows.net;SharedAccessKeyName=ReceiveOnlySharedAccessKey;SharedAccessKey=[your secret]" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
		<!-- Service Bus specific app setings for sending messaging connections -->
		<add key="Microsoft.ServiceBus.ConnectionString.Send" value="Endpoint=sb://[your namespace].servicebus.windows.net;SharedAccessKeyName=SendOnlySharedAccessKey;SharedAccessKey=[your secret]" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
	</appSettings>
</configuration>